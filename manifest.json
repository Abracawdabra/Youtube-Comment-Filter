{
    "manifest_version": 2,
    "name": "Youtube Comment Filter",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["*://*.youtube.com/*"]
        }
    ],
    "background": {
        "scripts":  ["background.js"]
    },
    "options_ui": {
        "page": "options.html"
    },
    "permissions": ["*://*.youtube.com/*", "storage", "tabs", "webNavigation"]
}
